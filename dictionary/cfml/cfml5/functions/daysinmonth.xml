<?xml version="1.0" encoding="UTF-8"?>

<!--
	Numeric DaysInMonth(date)
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="daysinmonth" returns="Numeric">
	<help><![CDATA[
		Determines the number of days in a month.
	]]></help>
	<parameter name="date" type="DateTime" required="true">
		<help><![CDATA[
		]]></help>
		<values>
			<value option="now()" />
		</values>
	</parameter>
</function>
