<?xml version="1.0" encoding="UTF-8"?>

<!--
	Numeric ListValueCountNoCase(list, value [, delimiters ])
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="listvaluecountnocase" returns="Numeric">
	<help><![CDATA[
		Counts instances of a specified value in a list. The search is
		case-insensitive.
	]]></help>
	<parameter name="list" type="String" required="true">
		<help><![CDATA[
		]]></help>
	</parameter>
	<parameter name="value" type="String" required="true">
		<help><![CDATA[
		]]></help>
	</parameter>
	<parameter name="delimiters" type="String" required="false">
		<help><![CDATA[
		]]></help>
		<values default=",">
			<value option="," />
			<value option="|" />
			<value option=";" />
			<value option="chr(9)" />
			<value option="chr(10)" />
			<value option="chr(13)" />
		</values>
	</parameter>
</function>
