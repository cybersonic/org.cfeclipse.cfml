<?xml version="1.0" encoding="UTF-8"?>

<!--
	String YesNoFormat(value)
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="yesnoformat" returns="String">
	<help><![CDATA[
		Yes, for a non-zero value; No, otherwise.
	]]></help>
	<parameter name="value" type="boolean" required="true">
		<help><![CDATA[
			A number or Boolean value
		]]></help>
		<values>
			<value option="true" />
			<value option="false" />
		</values>
	</parameter>
</function>
