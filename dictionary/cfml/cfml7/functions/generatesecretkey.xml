<?xml version="1.0" encoding="UTF-8"?>

<!--
	String GenerateSecretKey(algorithm)
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="generatesecretkey" returns="String">
	<help><![CDATA[
		Gets a secure key value for use in the encrypt function.
	]]></help>
	<parameter name="algorithm" type="String" required="false">
		<help><![CDATA[
		]]></help>
		<values>
			<value option="CFMX_COMPAT" />
			<value option="AES" />
			<value option="BLOWFISH" />
			<value option="DES" />
			<value option="DESEDE" />
		</values>
	</parameter>
</function>
