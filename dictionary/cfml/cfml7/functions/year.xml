<?xml version="1.0" encoding="UTF-8"?>

<!--
	Numeric Year(date)
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="year" returns="Numeric">
	<help><![CDATA[
		From a date/time object, gets the year value.
	]]></help>
	<parameter name="date" type="DateTime" required="true">
		<help><![CDATA[
		]]></help>
		<values>
			<value option="now()" />
		</values>
	</parameter>
</function>
