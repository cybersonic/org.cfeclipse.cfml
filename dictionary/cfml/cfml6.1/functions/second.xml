<?xml version="1.0" encoding="UTF-8"?>

<!--
	Numeric Second(date)
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="second" returns="Numeric">
	<help><![CDATA[
		Extracts the ordinal for the second from a date/time object.
	]]></help>
	<parameter name="date" type="DateTime" required="true">
		<help><![CDATA[
			A date/time object
		]]></help>
		<values>
			<value option="now()" />
		</values>
	</parameter>
</function>
