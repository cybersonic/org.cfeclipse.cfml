<?xml version="1.0" encoding="UTF-8"?>

<!--
	Object XmlNew([caseSensitive])
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="xmlnew" returns="Object">
	<help><![CDATA[
		Creates an XML document object.
	]]></help>
	<parameter name="casesensitive" type="boolean" required="false">
		<help><![CDATA[
			Maintains the case of document elements and attributes.
			Default: false
		]]></help>
		<values default="false">
			<value option="true" />
			<value option="false" />
		</values>
	</parameter>
</function>
