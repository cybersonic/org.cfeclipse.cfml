<?xml version="1.0" encoding="UTF-8"?>

<!--
	String ListRest(list [, delimiters ])
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="listrest" returns="String">
	<help><![CDATA[
		Gets a list, without its first element.
	]]></help>
	<parameter name="list" type="String" required="true">
		<help><![CDATA[
		]]></help>
	</parameter>
	<parameter name="delimiters" type="String" required="false">
		<help><![CDATA[
		]]></help>
		<values default=",">
			<value option="," />
			<value option="|" />
			<value option=";" />
			<value option="chr(9)" />
			<value option="chr(10)" />
			<value option="chr(13)" />
		</values>
	</parameter>
</function>
